red.":["\uad6c\uc131 \uc694\uc18c \uc774\ub984\uc740 \ud544\uc218\uc785\ub2c8\ub2e4."],"Component name already exists":["\uad6c\uc131 \uc694\uc18c \uc774\ub984\uc774 \uc774\ubbf8 \uc874\uc7ac\ud569\ub2c8\ub2e4"],"Component name is too long. Please keep it under 50 characters.":["\uad6c\uc131 \uc694\uc18c \uc774\ub984\uc774 \ub108\ubb34 \uae41\ub2c8\ub2e4. 50\uc790 \uc774\ub0b4\ub85c \uc720\uc9c0\ud574 \uc8fc\uc138\uc694."],"Failed to save component. Please try again.":["\uad6c\uc131 \uc694\uc18c\ub97c \uc800\uc7a5\ud558\uc9c0 \ubabb\ud588\uc2b5\ub2c8\ub2e4. \ub2e4\uc2dc \uc2dc\ub3c4\ud574 \uc8fc\uc138\uc694."],"Save as a component":["\uad6c\uc131 \uc694\uc18c\ub85c \uc800\uc7a5"],"Components":["\uad6c\uc131 \uc694\uc18c"],"Try something else.":["\ub2e4\ub978 \uac83\uc744 \uc2dc\ub3c4\ud574 \ubcf4\uc138\uc694."],"Clear & try again":["\ub2e4 \uc9c0\uc6b0\uace0 \ub2e4\uc2dc \uc2dc\ub3c4\ud558\uc138\uc694"],"Sorry, nothing matched":["\uc8c4\uc1a1\ud569\ub2c8\ub2e4. \uc77c\uce58\ud558\ub294 \ud56d\ubaa9\uc774 \uc5c6\uc2b5\ub2c8\ub2e4"],"Create":["\uc0dd\uc131"],"Update":["\uc5c5\ub370\uc774\ud2b8"],"Search":["\uac80\uc0c9"],"Close":["\ub2eb\uae30"],"Default":["\uae30\ubcf8"],"Name":["\uc774\ub984"],"Cancel":["\ucde8\uc18c"]}},"comment":{"reference":"assets\/js\/packages\/editor-components\/editor-components.strings.js"}}